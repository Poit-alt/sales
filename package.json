{
  "name": "sales",
  "version": "2.0.0",
  "description": "Sales app for tracking projects",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "NODE_ENV=development electron .",
    "build": "electron-builder",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "electron",
    "sales",
    "dashboard",
    "dark-mode"
  ],
  "author": "Peter Nordal Bj√∏rneseth",
  "license": "ISC",
  "devDependencies": {
    "electron": "^35.0.0",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "font-awesome": "^4.7.0"
  },
  "build": {
    "appId": "com.example.sales",
    "productName": "SalesVoyage",
    "icon": "src/icon.png",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.business"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Office"
    }
  }
}
